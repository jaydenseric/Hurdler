{
  "name": "hurdler",
  "version": "3.0.0-beta.6",
  "description": "Hurdler enables modular client-side URL hash routing.",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "http://jaydenseric.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydenseric/Hurdler.git"
  },
  "homepage": "https://github.com/jaydenseric/Hurdler#readme",
  "bugs": "https://github.com/jaydenseric/Hurdler/issues",
  "keywords": [
    "hurdler",
    "hurdles",
    "hurdle",
    "url",
    "hash",
    "router",
    "es6"
  ],
  "main": "dist/hurdler.js",
  "jsnext:main": "dist/hurdler.mjs",
  "scripts": {
    "lint": "eslint .",
    "prebuild": "npm run lint",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "docs": "jsdoc2md -t readme-template.hbs lib/**/*.js > readme.md",
    "prepublish": "npm run build && npm run docs"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "jsdoc-to-markdown": "^1.3.6",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-watch": "^2.5.0"
  },
  "babel": {
    "presets": [
      "es2015-rollup"
    ],
    "comments": false
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "prefer": {
            "return": "returns"
          }
        }
      ]
    }
  }
}
